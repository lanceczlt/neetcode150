class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        #brute force
        res = 0

        for i in range(len(s)):
            letters = set()
            for j in range(i, len(s)):
                if s[j] in letters:
                    break;
                letters.add(s[j])
            res = max(res, len(letters))
        return res

